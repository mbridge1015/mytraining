<% include ./common/header %>
<% include ./common/navbar %>
    <div class="container">
        <div class="starter-template">
            <h1>ブランチを登録します <input type="button" class="btn btn-primary btn-lg" value="Go Back" onClick="location.href='/git'"></h1>           
            <form class="form-horizontal" method="post" action="/gitNew/create">
                <div class="form-group">
                    <label class="col-md-3 control-label" for="inputtext">管理番号</label>
                    <div class="col-md-9">
                        <input type="text" name="title" class="form-control"></input>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label" for="inputtext">内容</label>
                    <div class="col-md-9">
                        <textarea rows="5" name="body" class="form-control"></textarea>
                    </div>
                </div>
                <input type="hidden" name="_csrf" value="<%= csrftoken %>">
                <input type="submit" class="btn btn-primary btn-lg" value="Add!">
            </form>
        </div>
    </div>
<% include ./common/footer %>